	$(srcdir)/ansi.c \
	$(srcdir)/charset.c \
	$(srcdir)/color.c \
	$(srcdir)/control.c \
	$(srcdir)/crum.c \
	$(srcdir)/edit.c \
	$(srcdir)/fun.c \
	$(srcdir)/init.c \
	$(srcdir)/menu.c \
	$(srcdir)/modes.c \
	$(srcdir)/output.c \
	$(srcdir)/pad.c \
	$(srcdir)/scan.c \
	$(srcdir)/sync.c \
	$(srcdir)/sysdep.c \
	$(srcdir)/tack.c

lintlib :
	@echo no action needed

# generated by mk-1st.awk (subset=termlib+ext_tinfo+base+ext_funcs)

